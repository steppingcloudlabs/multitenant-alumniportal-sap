{
    "welcomeFile": "/multitenant-alumniportal-sap-ui/index.html",
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/logout"
    },
    "routes": [{
        "source": "^//multitenant-alumniportal-sap-ui/(.*)$",
        "target": "$1",
        "localDir": "webapp"
    }, {
        "source": "^/(.*)$",
        "target": "$1",
        "csrfProtection": true,
        "authenticationType": "xsuaa",
        "destination": "dev-alumniportal-destination"
    }
]
}