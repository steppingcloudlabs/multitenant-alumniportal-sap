{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [
		{
			"source": "^/user/(.*)$",
			"authenticationType": "none",
			"destination": "single-tenant-alumniportal-sap-srv"
		},
		{
			"source": "^/admin/(.*)$",
			"authenticationType": "xsuaa",
			"scope": [
				"$XSAPPNAME.Administrator",
				"$XSAPPNAME.Manager"
			],
			"destination": "single-tenant-alumniportal-sap-srv"
		},
		{
			"source": "^/(.*)",
			"authenticationType": "none",
			"localDir": "resource"
		}
	]
}